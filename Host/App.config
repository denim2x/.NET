<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <connectionStrings>
    <add name="MyPhotosContainer"
      connectionString="metadata=res://*/MyPhotos.csdl|res://*/MyPhotos.ssdl|res://*/MyPhotos.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=(LocalDb)\MSSQLLocalDB;initial catalog=MyPhotos;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;"
      providerName="System.Data.EntityClient" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="ObjectWCF.MyPhotos" behaviorConfiguration="metadataSupport">
        <endpoint address="http://localhost:8000/MyPhotos" 
                  binding="basicHttpBinding" 
                  contract="ObjectWCF.IMyPhotos" 
                  name="basicHTTP_MyPhotos"> 
          <identity> 
            <dns value="localhost"/> 
          </identity> 


        </endpoint> 
        <endpoint address="mex" 
          binding="mexHttpBinding"  
          contract="IMetadataExchange"  
          name="mexhttp"/> 
      </service> 
    </services> 
    <behaviors> 
      <serviceBehaviors> 
        <behavior name="metadataSupport"> 
          <!-- Enables the IMetadataExchange endpoint in services that -->
          <!-- use "metadataSupport" in their behaviorConfiguration -->
          <!-- attribute. -->
          <!-- In addition, the httpGetEnabled and httpGetUrl -->
          <!-- attributes publish-->
          <!-- Service metadata for retrieval by HTTP/GET at the address -->
          <!-- "http://192.168.0.102:8000/SampleService?wsdl" -->
          <serviceMetadata httpGetEnabled="true" httpGetUrl=""/>
          <!-- <serviceMetadata/>-->
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>